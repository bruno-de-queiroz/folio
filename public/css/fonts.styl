domain = ""
@font-face
	font-family 'FontAwesome'
	font-weight normal
	font-style normal
	src url(domain'/fonts/fontawesome-webfont.eot')
	src url(domain'/fonts/fontawesome-webfont.eot#iefix&v=3.0.1') format('embedded-opentype'),
		url(domain'/fonts/fontawesome-webfont.woff') format('woff'),
		url(domain'/fonts/fontawesome-webfont.ttf') format('truetype')

